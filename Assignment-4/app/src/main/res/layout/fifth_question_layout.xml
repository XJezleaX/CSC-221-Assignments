<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/questionFifth"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="Which Street Fighter character wears a white outfit and a red headband? "
        android:textSize = "32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.539"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.24" />

    <Button
        android:id="@+id/fivebtnOne"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Akuma"
        android:textSize = "20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questionFifth"
        app:layout_constraintVertical_bias="0.245" />

    <Button
        android:id="@+id/fivebtnTwo"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Kage"
        android:textSize = "20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questionFifth"
        app:layout_constraintVertical_bias="0.367" />

    <Button
        android:id="@+id/fivebtnThree"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Ryu"
        android:textSize = "20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questionFifth"
        app:layout_constraintVertical_bias="0.494" />

    <Button
        android:id="@+id/fivebtnFour"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Juri"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questionFifth"
        app:layout_constraintVertical_bias="0.61" />

</androidx.constraintlayout.widget.ConstraintLayout>